.index_swiper swiper {
  width: 750rpx;
  height: 340rpx;
}
.index_swiper swiper image {
  width: 100%;
}
.index_nav {
  display: flex;
}
.index_nav view {
  flex: 1;
  padding: 15rpx;
}
.index_nav view image {
  width: 100%;
}
.index_floor .floor_group {
  overflow: hidden;
  padding: 10px;
}
.index_floor .floor_group .floor_title {
  display: flex;
  align-items: center;
  padding: 15rpx 0;
  background-color: #f4f4f4;
}
.index_floor .floor_group .floor_title image {
  width: 100%;
}
.index_floor .floor_group .floor_img {
  float: left;
  width: 33.3%;
}
.index_floor .floor_group .floor_img:nth-last-child(-n+4) {
  height: 27.70215517vw;
  border-left: 10rpx solid #fff;
}
.index_floor .floor_group .floor_img:nth-child(3),
.index_floor .floor_group .floor_img:nth-child(4) {
  border-bottom: 10rpx solid #fff;
}
.index_floor .floor_group .floor_img image {
  width: 100%;
  height: 100%;
}
